{
    "name": "edgardmessias/yii2-informix",
    "description": "Informix Driver for Yii2 Framework.",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "informix", "activerecord"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Edgard Messias",
            "email": "edgardmessias@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "yiisoft/yii2-dev": "2.0.14.2",
        "yiisoft/yii2-coding-standards": "^2.0.2",
        "phpunit/phpunit": "4.8.34"
    },
    "autoload": {
        "psr-4": {
            "edgardmessias\\db\\informix\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "platform": {
            "php": "5.4"
        }
    }
}
